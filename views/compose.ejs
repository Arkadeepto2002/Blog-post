<%- include("partials/header") %> 
<h1>Compose</h1>
<form method="post" action="/compose">
    <div class="form-group">
        <label for="postTitle">Title</label>
        <input type="text"  name="postTitle" class="form-control" id="postTitle" placeholder="Enter your title" autocomplete="off">
      </div>
      <div class="form-group">
        <label for="blog_post">Post</label>
        <textarea name="blog_post" placeholder="Write Here" autocomplete="off" rows="8" col="30" class="form-control" id="blog_post"></textarea>
      </div>
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer") %> 